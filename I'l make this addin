using System;

using Autodesk.Maya.Runtime;
using Autodesk.Maya.OpenMaya;

[assembly: MPxCommandClass(typeof(MayaNetPlugin.helloWorldCmd), "Cmd")]
[assembly: ExtensionPlugin(typeof(MayaNetPlugin.autofillPlugin), "Autodesk", "1.0", "Any")]

namespace MayaNetPlugin
{
    public class autofillPlugin : IExtensionPlugin
    {
        bool IExtensionPlugin.InitializePlugin()
        {
            return true;
        }

        bool IExtensionPlugin.UninitializePlugin()
        {
            return true;
        }
        String IExtensionPlugin.GetMayaDotNetSdkBuildVersion()
        {
            String version = "";
            return version;
        }

    }

    public class autofillCmd : MPxCommand,IMPxCommand
    {
        public override void doIt(MArgList argl)
        {
            MGlobal.displayInfo("autofill\n");
        }
    }
}
